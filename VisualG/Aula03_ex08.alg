algoritmo "semnome"
// Função :
// Autor :
// Data : 20/03/2019
// Seção de Declarações
var

media: real


inicio
// Seção de Comandos


se (media >= 9) entao
   escreva("otimo")


senao
   se (media >= 8) entao
      escreva("Bom")
   senao
      se (media >= 6) entao
         escreva("regular")
      senao
         escreva("Insuficiente") entao
      fimse
   fimse
fimse

fimalgoritmo
